3c len(A); i++ {\n\t\tA[i] *= A[i]\n\t}\n\tsort.Ints(A)\n\treturn A\n}\n\nfunc main() {\n\tA := []int{-4, -1, 0, 3, 10}\n\tresult := sortedSquares(A)\n\tfmt.Println(\"Sorted Squares:\", result)\n}\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20230905230143-xemyyla","Type":"NodeHeading","HeadingLevel":2,"Properties":{"id":"20230905230143-xemyyla","updated":"20230905230148"},"Children":[{"Type":"NodeText","Data":"双指针法"}]},{"ID":"20230905230200-swp4zi9","Type":"NodeParagraph","Properties":{"id":"20230905230200-swp4zi9","updated":"20230905230200"},"Children":[{"Type":"NodeText","Data":"数组其实是有序的， 只不过负数平方之后可能成为最大数了。"}]},{"ID":"20230905230200-tyl942z","Type":"NodeParagraph","Properties":{"id":"20230905230200-tyl942z","updated":"20230905230200"},"Children":[{"Type":"NodeText","Data":"那么数组平方的最大值就在数组的两端，不是最左边就是最右边，不可能是中间。"}]},{"ID":"20230905230200-3gz3oau","Type":"NodeParagraph","Properties":{"id":"20230905230200-3gz3oau","updated":"20230905230200"},"Children":[{"Type":"NodeText","Data":"此时可以考虑双指针法了，i指向起始位置，j指向终止位置。"}]},{"ID":"20230905230200-wi44lsq","Type":"NodeParagraph","Properties":{"id":"20230905230200-wi44lsq","updated":"20230905230200"},"Children":[{"Type":"NodeText","Data":"定义一个新数组result，和A数组一样的大小，让k指向result数组终止位置。"}]},{"ID":"20230905230200-poexuxt","Type":"NodeParagraph","Properties":{"id":"20230905230200-poexuxt","updated":"20230905230200"},"Children":[{"Type":"NodeText","Data":"如果"},{"Type":"NodeTextMark","TextMarkType":"code","TextMarkTextContent":"A[i] * A[i] \u0026lt; A[j] * A[j]"},{"Type":"NodeText","Data":"​ 那么"},{"Type":"NodeTextMark","TextMarkType":"code","TextMarkTextContent":"result[k--] = A[j] * A[j];"},{"Type":"NodeText","Data":"​ 。"}]},{"ID":"20230905230200-pt32r7e","Type":"NodeParagraph","Properties":{"id":"20230905230200-pt32r7e","updated":"20230905230200"},"Children":[{"Type":"NodeText","Data":"如果"},{"Type":"NodeTextMark","TextMarkType":"code","TextMarkTextContent":"A[i] * A[i] \u0026gt;= A[j] * A[j]"},{"Type":"NodeText","Data":"​ 那么"},{"Type":"NodeTextMark","TextMarkType":"code","TextMarkTextContent":"result[k--] = A[i] * A[i];"},{"Type":"NodeText","Data":"​ 。"}]},{"ID":"20230905230200-d95rrlj","Type":"NodeParagraph","Properties":{"id":"20230905230200-d95rrlj","updated":"20230905230221"},"Children":[{"Type":"NodeText","Data":"如动画所示："}]},{"ID":"20230905230225-a4tczej","Type":"NodeParagraph","Properties":{"id":"20230905230225-a4tczej"},"Children":[{"Type":"NodeText","Data":"​"},{"Type":"NodeImage","Data":"span","Children":[{"Type":"NodeBang"},{"Type":"NodeOpenBracket"},{"Type":"NodeLinkText","Data":"977.有序数组的平方"},{"Type":"NodeCloseBracket"},{"Type":"NodeOpenParen"},{"Type":"NodeLinkDest","Data":"assets/977.有序数组的平方-20230905230221-i7c0n0o.gif"},{"Type":"NodeCloseParen"}]},{"Type":"NodeText","Data":"​"}]},{"ID":"20230905230238-1rfk7x0","Type":"NodeParagraph","Properties":{"id":"20230905230238-1rfk7x0","updated":"20230905230246"},"Children":[{"Type":"NodeText","Data":"不难写出如下代码"}]},{"ID":"20230905230255-nju2usv","Type":"NodeCodeBlock","IsFencedCodeBlock":true,"Properties":{"id":"20230905230255-nju2usv","updated":"20230905230506"},"Children":[{"Type":"NodeCodeBlockFenceOpenMarker","Data":"```"},{"Type":"NodeCodeBlockFenceInfoMarker","CodeBlockInfo":"Z28="},{"Type":"NodeCodeBlockCode","Data":"func sortedSquares(nums []int) []int {\n    n := len(nums)\n    i, j, k := 0, n-1, n-1\n    ans := make([]int, n)\n    for i \u003c= j {\n        lm, rm := nums[i]*nums[i], nums[j]*nums[j]\n        if lm \u003e rm {\n            ans[k] = lm\n            i++\n        } else {\n            ans[k] = rm\n            j--\n        }\n        k--\n    }\n    return ans\n}\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20230905230255-fiqghls","Type":"NodeParagraph","Properties":{"id":"20230905230255-fiqghls","updated":"20230905230303"},"Children":[{"Type":"NodeText","Data":"此时的时间复杂度为O(n)，相对于暴力排序的解法O(n + nlog n)还是提升不少的。"}]},{"ID":"20230905230257-ognowfu","Type":"NodeParagraph","Properties":{"id":"20230905230257-ognowfu"}}]}